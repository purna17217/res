package com.tcs.bancs.microservices.utils;

public class TellerResult {

    private final boolean allowTeller;
    private final boolean validTeller;
    private final boolean tableValueEmpty;

    public TellerResult(boolean allowTeller, boolean validTeller, boolean tableValueEmpty) {
        this.allowTeller = allowTeller;
        this.validTeller = validTeller;
        this.tableValueEmpty = tableValueEmpty;
    }

    public boolean isAllowTeller() {
        return allowTeller;
    }

    public boolean isValidTeller() {
        return validTeller;
    }

    public boolean isTableValueEmpty() {
        return tableValueEmpty;
    }
}





